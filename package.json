{
  "author": "Mateusz Janota <m.janota@zanreal.com>",
  "bugs": {
    "url": "https://github.com/z4nr34l/nemo/issues"
  },
  "dependencies": {
    "@fumadocs/mdx-remote": "^1.2.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.12",
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "eslint": "^9",
    "eslint-config-next": "16.0.3",
    "husky": "^9.1.7",
    "turbo": "^2.5.0"
  },
  "homepage": "https://nemo.zanreal.com",
  "keywords": [
    "Next.js",
    "Middleware",
    "Multiple middleware",
    "Pathname",
    "Route",
    "Path"
  ],
  "license": "MIT",
  "name": "nemo",
  "packageManager": "bun@1.3.2",
  "publishConfig": {
    "provenance": true
  },
  "repository": "https://github.com/z4nr34l/nemo",
  "scripts": {
    "build": "turbo build",
    "clean": "turbo clean",
    "dev": "turbo dev --concurrency 20",
    "examples": "turbo examples",
    "prepare": "husky",
    "release": "turbo build --filter='@rescale/nemo' && changeset publish",
    "version-packages": "changeset version"
  },
  "trustedDependencies": [
    "@swc/core",
    "docs"
  ],
  "workspaces": [
    "packages/*",
    "examples/*",
    "apps/*"
  ]
}