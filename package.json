{
  "author": "Z4NR34L <software@zanreal.net>",
  "bugs": {
    "url": "https://github.com/z4nr34l/nemo/issues"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.12",
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "husky": "^9.1.7",
    "turbo": "^2.4.4"
  },
  "homepage": "https://nemo.rescale.build",
  "keywords": [
    "Next.js",
    "Middleware",
    "Multiple middleware",
    "Pathname",
    "Route",
    "Path"
  ],
  "license": "MIT",
  "name": "root",
  "packageManager": "bun@1.2.2",
  "publishConfig": {
    "provenance": true
  },
  "repository": "https://github.com/z4nr34l/nemo",
  "scripts": {
    "build": "turbo build",
    "clean": "turbo clean",
    "dev": "turbo dev --concurrency 20",
    "examples": "turbo examples",
    "prepare": "husky",
    "release": "turbo build --filter='@rescale/nemo' && changeset publish",
    "version-packages": "changeset version"
  },
  "workspaces": [
    "packages/*",
    "examples/*",
    "apps/*"
  ],
  "dependencies": {
    "@fumadocs/mdx-remote": "^1.2.0"
  }
