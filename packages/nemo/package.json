{
  "name": "@rescale/nemo",
  "version": "2.1.0",
  "keywords": [
    "Next.js",
    "Middleware",
    "Multiple middleware",
    "Pathname",
    "Route",
    "Path"
  ],
  "homepage": "https://nemo.zanreal.com",
  "bugs": {
    "url": "https://github.com/z4nr34l/nemo/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/z4nr34l/nemo.git",
    "directory": "packages/nemo"
  },
  "license": "MIT",
  "author": "Mateusz Janota <m.janota@zanreal.com>",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "package.json",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsup --clean --splitting",
    "clean": "rm -rf node_modules && rm -rf .turbo && rm -rf dist",
    "dev": "tsup --watch",
    "lint": "eslint \"**/*.ts*\"",
    "test": "jest --coverage"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./storage": {
      "types": "./dist/storage/index.d.ts",
      "require": "./dist/storage/index.js",
      "import": "./dist/storage/index.js"
    },
    "./storage/adapters/memory": {
      "types": "./dist/storage/adapters/memory/index.d.ts",
      "require": "./dist/storage/adapters/memory/index.js",
      "import": "./dist/storage/adapters/memory/index.js"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "path-to-regexp": "^6.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@next/eslint-plugin-next": "^15.1.0",
    "@swc/core": "^1.5.24",
    "@types/bun": "^1.2.2",
    "@types/node": "^22.13.1",
    "@types/react": "^19.0.1",
    "eslint": "^9",
    "eslint-config-next": "16.0.3",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-prettier": "^5.2.2",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-turbo": "^2.3.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^3.0.2",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "tsup": "^8.1.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.0"
  },
  "peerDependencies": {
    "next": ">=13.0.0"
  },
  "publishConfig": {
    "provenance": true
  }
}
